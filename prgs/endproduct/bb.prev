fun putInt(a:int):void;
fun putChar(c:char):void;
fun putString(s:ptr char):void;

fun main():int=
    { f=1; n=1; i=1;
      while n<=10 do
      i = n;
      f = 1;
      while i>0 do f=f*i; i=i-1; end;
      putInt(f); putChar((10 : char));
      n = n+1;
      end;
    : f
    where
      var f:int;
      var n:int;
      var i:int;
    };
